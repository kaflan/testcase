{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "no-console": "off",
    "semi": 2,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-no-target-blank": "off",
    "react/no-array-index-key": "off",
    "react/jsx-props-no-spreading": "off",
    "prettier/prettier": [
      "error", {
        "semi": true,
        "tabWidth" : 2,
        "singleQuote": true
      }
    ]
  }
}
